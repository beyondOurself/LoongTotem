{
  "private": "true",
  "version": "0.0.0",
  "files": [
    "lib"
  ],
  "main": "./lib/index.umd.js",
  "module": "./lib/index.es.js",
  "exports": {
    ".": {
      "import": "./lib/index.es.js",
      "require": "./lib/index.umd.js"
    }
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build --watch",
    "preview": "vite preview",
    "gen": "bash ./scripts/gc.sh",
    "clean:lib": "rimraf lib",
    "build:theme": "rimraf packages/loong-totem-view-theme/lib && gulp build --gulpfile packages/loong-totem-view-theme/gulpfile.js && cp-cli packages/loong-totem-view-theme/lib lib/long-view-theme && rimraf packages/long-view-theme/lib",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "config:test": "vite --config test.vite.config.ts",
    "config:build": "vue-tsc --noEmit && vite build -config test.vite.config.ts"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.0",
    "@vuepress/plugin-container": "^2.0.0-beta.32",
    "@vuepress/plugin-register-components": "^2.0.0-beta.32",
    "@vuepress/plugin-shiki": "^2.0.0-beta.32",
    "clipboard": "^2.0.8",
    "cp-cli": "^2.0.0",
    "gulp": "^4.0.2",
    "lerna": "^4.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.47.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8",
    "vuepress": "^2.0.0-beta.32"
  }
}
